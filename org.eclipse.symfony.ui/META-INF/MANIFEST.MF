Manifest-Version: 1.0
Bundle-ManifestVersion: 2
Bundle-Name: Symfony UI Plugin
Bundle-SymbolicName: org.eclipse.symfony.ui;singleton:=true
Bundle-Version: 1.0.2.qualifier
Bundle-Activator: org.eclipse.symfony.ui.SymfonyUiPlugin
Require-Bundle: org.eclipse.ui,
